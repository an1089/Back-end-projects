<!DOCTYPE html>
<html>
    <head>
        <title>check</title>
        <link rel="stylesheet" href="css/styles.css">
    </head>
    <body>
      <div class="box" id="heading">
    <h1> <%= ListTitle %> </h1>
  </div>

    <div class="box">

    <% newListItems.forEach(function(item) { %>
      <div class="item">
        <input type="checkbox">
     <p><%= item.name %></p>
   </div>
   <% }) %>


    <form class="item" action="/" method="post">
     <input type="text" name="list" placeholder="New Item">
     <button type="submit" name="button">+</button>
  </form>
</div>
</body>
</html>
